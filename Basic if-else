#include <stdio.h>

int main() {

    int x;
    
    printf("Type an integer:");
    scanf("%d", &x);
    if(x>10)
    {
        printf("You typed %d.\n",x);
        printf("%d is greater than 10.\n", x);
    }   
    else
    {
        printf("You typed %d.\n",x);
        printf("%d is less than or equal to 10.\n", x);
    }

    return 0;
}
